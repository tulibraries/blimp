<p id="notice"><%= notice %></p>

<h1>Uploaded files</h1>

<table>
  <thead>
    <tr>
      <th>Map filename</th>
      <th>Id field</th>
      <th>Datafile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @uploads.each do |upload| %>
      <tr>
        <td><%= upload.map_filename %></td>
        <td><%= upload.id_field %></td>
        <td><%= upload.datafile %></td>
        <td><%= link_to 'Show', upload %></td>
        <td><%= link_to 'Edit', edit_upload_path(upload) %></td>
        <td><%= link_to 'Destroy', upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload', new_upload_path %>
